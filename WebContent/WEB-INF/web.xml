<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>AgendaHibernate</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <jsp-config>
    <taglib>
      <taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
      <taglib-location>c.tld</taglib-location>
    </taglib>
  </jsp-config>
  <servlet>
    <servlet-name>AutenticarUsuario</servlet-name>
    <servlet-class>org.pablofranco.agenda.servlet.AutenticarUsuario</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AutenticarUsuario</servlet-name>
    <url-pattern>/AutenticarUsuario.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletCerrarSesion</servlet-name>
    <servlet-class>org.pablofranco.agenda.servlet.ServletCerrarSesion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletCerrarSesion</servlet-name>
    <url-pattern>/ServletCerrarSesion.do</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>ServletAgregarContacto</servlet-name>
    <servlet-class>org.pablofranco.agenda.servlet.ServletAgregarContacto</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletAgregarContacto</servlet-name>
    <url-pattern>/ServletAgregarContacto.do</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>ServletCargarContacto</servlet-name>
    <servlet-class>org.pablofranco.agenda.servlet.ServletCargarContacto</servlet-class>
