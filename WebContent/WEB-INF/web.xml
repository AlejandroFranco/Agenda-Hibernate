    <servlet-name>ServletEditarContacto</servlet-name>
    <url-pattern>/ServletEditarContacto.do</url-pattern>
  </servlet-mapping>
    <servlet>
  <servlet-name>ServletEliminarContacto</servlet-name>
    <servlet-class>org.pablofranco.agenda.servlet.ServletEliminarContacto</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletEliminarContacto</servlet-name>
    <url-pattern>/ServletEliminarContacto.do</url-pattern>
  </servlet-mapping>
  
    <servlet>
  <servlet-name>ServletListarContactos</servlet-name>
    <servlet-class>org.pablofranco.agenda.servlet.ServletListarContactos</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletListarContactos</servlet-name>
    <url-pattern>/ServletListarContactos.do</url-pattern>
  </servlet-mapping>

</web-app>